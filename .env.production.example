# База данных PostgreSQL (для продакшена)
DATABASE_URL="postgresql://shoppinglist:YOUR_PASSWORD@postgres:5432/shoppinglist"

# JWT Secret - ОБЯЗАТЕЛЬНО смените на надежный случайный ключ!
# Сгенерировать можно командой: openssl rand -base64 32
JWT_SECRET="change-this-to-a-secure-random-key-in-production"

# Настройки PostgreSQL
POSTGRES_USER="shoppinglist"
